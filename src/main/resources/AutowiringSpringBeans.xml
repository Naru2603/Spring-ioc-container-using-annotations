<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
 <context:annotation-config/>

	<bean id="car" class="com.infosys.model.Car">
		<property name="model" value="2016"></property>
		<property name="company" value="Audi"></property>
	</bean>
	
	<bean id="engine" class="com.infosys.model.Engine">
		<property name="capacity" value="2500"></property>
		<property name="type" value="V12"></property>
	</bean>
	
	<context:component-scan base-package="com.infosys.model"/>
</beans>