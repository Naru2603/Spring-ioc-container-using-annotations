<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />
	<context:component-scan base-package="com.infosys.AutowireAnnotations" />
	<bean id="employee" class="com.infosys.AutowireAnnotations.Employee">
		<property name="id" value="214"></property>
		<property name="name" value="Rahul"></property>
	</bean>

	<bean id="department" class="com.infosys.AutowireAnnotations.Department">
		<property name="deptId" value="2"></property>
		<property name="name" value="HR-department"></property>
	</bean>
<bean id="department1" class="com.infosys.AutowireAnnotations.Department">
		<property name="deptId" value="2"></property>
		<property name="name" value="QA-department"></property>
	</bean>

</beans>